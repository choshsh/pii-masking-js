{
  "name": "@cho_o/pii-masking-js",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.0.1",
  "description": "PII(개인식별정보) 마스킹을 위한 경량 라이브러리",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc --module commonjs --outDir dist --declaration false --removeComments",
    "build:esm": "tsc --module esnext --outDir dist-esm --removeComments && node scripts/rename-esm.js",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "test": "node --test test/accountNumber.test.js test/address.test.js test/cardNumber.test.js test/email.test.js test/phone.test.js test/rnn.test.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "pii",
    "masking",
    "privacy",
    "security",
    "personal-information",
    "data-protection"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.18.63",
    "typescript": "^4.9.5"
  }
}
